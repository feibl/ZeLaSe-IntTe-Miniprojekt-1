<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>user</managed-bean-name>
		<managed-bean-class>chatlobby.User</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>name</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>chat</managed-bean-name>
		<managed-bean-class>chatlobby.Chat</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>theme</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>chatManagerBean</managed-bean-name>
		<managed-bean-class>chatlobby.ChatManagerBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>userManager</managed-bean-name>
		<managed-bean-class>chatlobby.UserManager</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>registrationFormular</managed-bean-name>
		<managed-bean-class>chatlobby.RegistrationFormular</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>password</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>pw_verification</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>username</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
	</managed-bean>

</faces-config>
